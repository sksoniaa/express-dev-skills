<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Skills</title>
</head>
<body>
  <h1>Skills</h1>
  <ul>
    <% skills.forEach(function(s) { %>
      <li>
        <a href="/skills/<%= s.id %>"><%= s.skill %></a>
          - 
        <%= s.done ? 'done' : 'not done' %>
      </li>
    <% }); %>
  </ul>

  <h3><%= skill.skill %></h3>
  <h3>Complete:<%= skill.done ? 'Yes' : 'No' %></h3>
</body>
</html>